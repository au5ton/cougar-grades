{
  "name": "cougar-grades",
  "version": "1.0.0",
  "description": "Web application",
  "dependencies": {
    "better-sqlite3": "^5.4.0",
    "chalk": "^2.4.2",
    "convert-hrtime": "^3.0.0",
    "dotenv": "^8.0.0",
    "fastify": "^2.3.0",
    "fastify-cli": "^1.0.0",
    "fastify-graceful-shutdown": "^2.0.1",
    "fastify-rate-limit": "^2.1.1",
    "fastify-response-time": "github:lolo32/fastify-response-time",
    "fastify-static": "^2.4.0",
    "handlebars": "^4.1.2",
    "make-promises-safe": "^5.0.0",
    "mysql2": "^1.6.5",
    "node-fetch": "^2.5.0",
    "pretty-hrtime": "^1.0.3",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.0",
    "mustache": "^3.0.1",
    "sass": "^1.20.1"
  },
  "config": {
    "server": {
      "port": 3000
    }
  },
  "scripts": {
    "start": "npm run prod",
    "prod": "npm run prod:build; npm run prod:server",
    "prod:server": "node server.js",
    "prod:build": "./front/preprocess.sh",
    "dev": "npm run dev:mustache & npm run dev:sass & npm run dev:nodemon",
    "dev:nodemon": "npx nodemon --exec node --inspect=0.0.0.0:9229 server.js",
    "dev:mustache": "npx nodemon --watch front/mustache/ --ext mustache --exec ./front/generate_html.js",
    "dev:sass": "npx sass --watch ./front/sass:./front/_site/css/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/au5ton/cougar-grades.git"
  },
  "license": "ISC"
}
