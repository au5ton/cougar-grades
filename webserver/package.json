{
  "name": "cougar-grades",
  "version": "1.0.0",
  "description": "Web application",
  "dependencies": {
    "better-sqlite3": "^5.4.0",
    "chalk": "^2.4.2",
    "convert-hrtime": "^3.0.0",
    "fastify": "^2.3.0",
    "fastify-cli": "^1.0.0",
    "fastify-graceful-shutdown": "^2.0.1",
    "fastify-rate-limit": "^2.1.1",
    "fastify-response-time": "github:lolo32/fastify-response-time",
    "fastify-static": "^2.4.0",
    "make-promises-safe": "^5.0.0",
    "mustache": "^3.0.1",
    "mysql2": "^1.6.5",
    "node-fetch": "^2.5.0",
    "point-of-view": "^3.3.0",
    "pretty-hrtime": "^1.0.3",
    "redis": "^2.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.0",
    "sass": "^1.20.1",
    "standard": "*"
  },
  "config": {
    "sass": {
      "src": "assets/sass/",
      "dest": "assets/public/css/"
    },
    "server": {
      "port": 3000
    }
  },
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "npm run build:sass",
    "build:sass": "npx sass --update $npm_package_config_sass_src:$npm_package_config_sass_dest",
    "server": "node server.js",
    "dev": "npm run dev:sass & npm run dev:nodemon",
    "dev:sass": "npx sass --watch $npm_package_config_sass_src:$npm_package_config_sass_dest",
    "dev:nodemon": "npx nodemon --ignore assets/ --exec node --inspect=0.0.0.0:9229 server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/au5ton/cougar-grades.git"
  },
  "license": "ISC"
}
